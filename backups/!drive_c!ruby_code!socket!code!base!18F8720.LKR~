SECTION   	NAME=highIntVector_CODE 			ROM=_highIntVector_CODE

CODEPAGE  	NAME=_bootloadPriVer_ROMDATA		START=0x10									END=0x17			PROTECTED	FILL=0xFFFF 						// primary bootload version 
SECTION   	NAME=bootloadPriVer_ROMDATA 		ROM=_bootloadPriVer_ROMDATA

CODEPAGE  	NAME=_lowIntVector_CODE  			START=0x18									END=0x1F			PROTECTED	FILL=0xFFFF
SECTION   	NAME=lowIntVector_CODE 				ROM=_lowIntVector_CODE

CODEPAGE  	NAME=_bootload_CODE					START=0x20									END=0x1FF			PROTECTED	FILL=0xFFFF 						// Needs to be consistent with factory bootloader
SECTION	  	NAME=bootload_CODE   				ROM=_bootload_CODE

